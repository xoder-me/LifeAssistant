<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

	<include
		layout="@layout/item_mine_top"/>

	<RelativeLayout
		android:id="@+id/ll_express_mine"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:layout_below="@+id/rl_mine_query"
		android:orientation="vertical">

		<ImageView
			android:id="@+id/iv_mine_not_found"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_below="@+id/lv_express_mine"
			android:layout_centerInParent="true"
			android:src="@drawable/express_not_found"
			android:visibility="gone"/>

		<TextView
			android:id="@+id/tv_mine_not_found"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_below="@+id/iv_mine_not_found"
			android:layout_centerHorizontal="true"
			android:text="亲爱哒, 就搜一下下嘛..."
			android:textColor="#BDBFC1"
			android:textSize="@dimen/font_size_big"
			android:visibility="gone"/>

		<ListView
			android:id="@+id/lv_express_mine"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:visibility="gone"/>
	</RelativeLayout>
</RelativeLayout>